{
  "name": "aliem",
  "version": "0.8.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run lint",
    "lint":
      "tslint -p . -t codeFrame && php-cs-fixer fix --config=.php_cs -vvv --dry-run --stop-on-violation --using-cache=no",
    "phpcsfix": "php-cs-fixer fix --config=.php_cs -v",
    "start": "gulp",
    "build": "NODE_ENV=production gulp",
    "torus": "torus run 'echo test' &>/dev/null || torus login",
    "deploy": "npm run torus && torus run ./lib/scripts/deploy",
    "deploy-all": "npm run torus && torus run ./lib/scripts/deploy -a",
    "deploy-initial": "npm run torus && torus run ./lib/scripts/deploy -ai",
    "sync": "./lib/scripts/sync $@",
    "version": "gulp bump && git add -A"
  },
  "dependencies": {
    "intersection-observer": "^0.4.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-virtualized": "^9.12.0",
    "recharts": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "@types/gulp-autoprefixer": "0.0.31",
    "@types/gulp-rename": "0.0.33",
    "@types/gulp-sass": "0.0.31",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/node": "^8.0.50",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-virtualized": "^9.7.6",
    "@types/webpack": "^3.8.1",
    "awesome-typescript-loader": "^3.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "css-loader": "^0.28.7",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "merge-stream": "^1.0.1",
    "node-sass": "^4.6.0",
    "prettier": "^1.8.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "lint-staged": {
    "*.{js,json,scss,ts,tsx}": ["prettier --write", "git add"],
    "*.php": ["php-cs-fixer fix --config=.php_cs -v", "git add"]
  }
}
